{
  "name": "1j1s-orchestrateur-transform-load",
  "version": "0.1.0",
  "description": "Dépôt qui regroupe les opérations de transformation et de chargement des données issues de flux pour les stages du site 1j1s",
  "main": "start.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "cli": "node --max-old-space-size=512 ./build/src/cli",
    "coverage": "nyc mocha",
    "lint": "eslint src test",
    "start": "node build/src/start.js",
    "test": "mocha"
  },
  "_moduleAliases": {
    "@root": "./",
    "@configuration": "build/src/configuration",
    "@chargement": "build/src/chargement",
    "@extraction": "build/src/extraction",
    "@shared": "build/src/shared",
    "@transformation": "build/src/transformation"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load.git"
  },
  "author": "OCTO Technology",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load/issues"
  },
  "homepage": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load#readme",
  "dependencies": {
    "axios": "~0.27.2",
    "dotenv": "~16.0.1",
    "fast-xml-parser": "~4.0.8",
    "i18n-iso-countries": "~7.5.0",
    "minio": "~7.0.28",
    "module-alias": "~2.2.2",
    "pino": "~8.4.2",
    "pino-sentry": "^0.13.0",
    "turndown": "~7.1.1",
    "yargs": "~17.5.1"
  },
  "devDependencies": {
    "@salesforce/ts-sinon": "~1.3.21",
    "@types/chai": "~4.3.1",
    "@types/chai-as-promised": "~7.1.5",
    "@types/minio": "~7.0.13",
    "@types/mocha": "~9.1.1",
    "@types/module-alias": "~2.0.1",
    "@types/node": "~18.7.14",
    "@types/sinon": "~10.0.12",
    "@types/sinon-chai": "~3.2.8",
    "@types/turndown": "~5.0.1",
    "@types/yargs": "~17.0.10",
    "@typescript-eslint/eslint-plugin": "~5.36.0",
    "@typescript-eslint/parser": "~5.36.0",
    "chai": "~4.3.6",
    "chai-as-promised": "~7.1.1",
    "eslint": "~8.23.0",
    "mocha": "~10.0.0",
    "nock": "~13.2.9",
    "nyc": "~15.1.0",
    "sinon": "~14.0.0",
    "sinon-chai": "~3.7.0",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.1.0",
    "typescript": "~4.8.2"
  },
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/test/**/*.ts",
      "**/configuration/**/*.ts",
      "**/start.ts",
      "**/setup.ts",
      "**/src/**/tasks/*",
      "**/infrastructure/**/node-file-system.client.ts",
      "**/infrastructure/**/country-to-iso.ts",
      "**/infrastructure/**/html-to-markdown.sanitizer.ts",
      "**/infrastructure/**/uuid.generator.ts",
      "**/infrastructure/**/unzip.client.ts",
      "**/src/cli/**/*.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
