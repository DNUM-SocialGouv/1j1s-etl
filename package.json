{
  "name": "1j1s-etl",
  "version": "4.0.0",
  "description": "Dépôt qui regroupe les opérations de transformation et de chargement des données issues de flux pour les stages du site 1j1s",
  "main": "start.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "nest build cli && nest build api",
    "build:api": "nest build api",
    "build:cli": "nest build cli",
    "cli": "node build/apps/cli/apps/cli/src/main.js",
    "coverage": "nyc mocha",
    "dev:api": "nest build api && nest start api",
    "dev:cli": "nest build cli && node -r dotenv/config build/apps/cli/apps/cli/src/main.js",
    "lint": "eslint apps",
    "lint:fix": "eslint apps --fix",
    "prepare": "./setup.sh",
    "start": "nest start api",
    "test": "mocha",
    "test:mutation": "npx stryker run",
    "test:mutation:prepare": "tsc -p tsconfig.mutation.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load.git"
  },
  "author": "OCTO Technology",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load/issues"
  },
  "homepage": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load#readme",
  "dependencies": {
    "@nestjs/common": "~9.3.9",
    "@nestjs/core": "~9.3.9",
    "@nestjs/platform-express": "~9.3.9",
    "axios": "~1.3.3",
    "basic-ftp": "~5.0.2",
    "fast-xml-parser": "~4.1.2",
    "i18n-iso-countries": "~7.5.0",
    "luxon": "~3.2.1",
    "meilisearch": "~0.31.1",
    "minio": "~7.0.28",
    "nest-commander": "~3.5.0",
    "node-stream-zip": "~1.15.0",
    "pino": "~8.10.0",
    "pino-sentry": "~0.14.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.8.0",
    "source-map-support": "~0.5.21",
    "turndown": "~7.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "~9.2.0",
    "@nestjs/testing": "~9.3.9",
    "@salesforce/ts-sinon": "~1.4.6",
    "@sefr/eslint-plugin": "~1.3.2",
    "@stryker-mutator/core": "~6.4.1",
    "@stryker-mutator/mocha-runner": "~6.4.1",
    "@stryker-mutator/typescript-checker": "~6.4.1",
    "@types/chai": "~4.3.1",
    "@types/chai-as-promised": "~7.1.5",
    "@types/luxon": "~3.2.0",
    "@types/minio": "~7.0.14",
    "@types/mocha": "~10.0.0",
    "@types/node": "~18.14.0",
    "@types/sinon": "~10.0.12",
    "@types/sinon-chai": "~3.2.8",
    "@types/supertest": "~2.0.12",
    "@types/turndown": "~5.0.1",
    "@typescript-eslint/eslint-plugin": "~5.52.0",
    "@typescript-eslint/parser": "~5.52.0",
    "chai": "~4.3.6",
    "chai-as-promised": "~7.1.1",
    "dotenv": "~16.0.3",
    "eslint": "~8.34.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "git-notify": "~0.2.3",
    "mocha": "~10.2.0",
    "nock": "~13.3.0",
    "nyc": "~15.1.0",
    "sinon": "~15.0.0",
    "sinon-chai": "~3.7.0",
    "supertest": "~6.3.3",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5"
  },
  "engines": {
    "node": "18.14.0",
    "npm": "9.3.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/test/**/*.ts",
      "**/configuration/**/*.ts",
      "**/start.ts",
      "**/setup.ts",
      "**/src/**/tasks/*",
      "**/src/evenements/index.ts",
      "**/src/logements/index.ts",
      "**/src/stages/index.ts",
      "**/infrastructure/**/node-file-system.client.ts",
      "**/infrastructure/**/country-to-iso.ts",
      "**/infrastructure/**/html-to-markdown.sanitizer.ts",
      "**/infrastructure/**/uuid.generator.ts",
      "**/infrastructure/**/unzip.client.ts",
      "**/infrastructure/**/feature-flipping-*.ts",
      "**/src/cli/**/*.ts",
      "**/tool/*"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
