{
  "name": "1j1s-etl",
  "version": "3.4.4",
  "description": "Dépôt qui regroupe les opérations de transformation et de chargement des données issues de flux pour les stages du site 1j1s",
  "main": "start.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "cli": "node --max-old-space-size=512 ./build/src/cli",
    "coverage": "nyc mocha",
    "dev": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "dev:cli": "npm run dev -- src/cli",
    "dev:start": "npm run dev -- src/start.ts",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "prepare": "./setup.sh",
    "start": "node build/src/start.js",
    "test": "mocha",
    "test:mutation": "npx stryker run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load.git"
  },
  "author": "OCTO Technology",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load/issues"
  },
  "homepage": "https://github.com/DNUM-SocialGouv/1j1s-orchestrateur-transform-load#readme",
  "dependencies": {
    "axios": "~1.3.2",
    "basic-ftp": "~5.0.2",
    "fast-xml-parser": "~4.1.1",
    "i18n-iso-countries": "~7.5.0",
    "luxon": "~3.2.1",
    "minio": "~7.0.28",
    "node-stream-zip": "~1.15.0",
    "pino": "~8.8.0",
    "pino-sentry": "~0.14.0",
    "turndown": "~7.1.1",
    "yargs": "~17.6.1"
  },
  "devDependencies": {
    "@salesforce/ts-sinon": "~1.4.4",
    "@sefr/eslint-plugin": "~1.3.1",
    "@stryker-mutator/core": "~6.3.1",
    "@stryker-mutator/mocha-runner": "~6.3.1",
    "@stryker-mutator/typescript-checker": "~6.3.1",
    "@types/chai": "~4.3.1",
    "@types/chai-as-promised": "~7.1.5",
    "@types/eslint": "~8.21.0",
    "@types/estree": "~1.0.0",
    "@types/luxon": "~3.2.0",
    "@types/minio": "~7.0.14",
    "@types/mocha": "~10.0.0",
    "@types/module-alias": "~2.0.1",
    "@types/node": "~18.11.9",
    "@types/sinon": "~10.0.12",
    "@types/sinon-chai": "~3.2.8",
    "@types/turndown": "~5.0.1",
    "@types/yargs": "~17.0.22",
    "@typescript-eslint/eslint-plugin": "~5.50.0",
    "@typescript-eslint/parser": "~5.50.0",
    "chai": "~4.3.6",
    "chai-as-promised": "~7.1.1",
    "dotenv": "~16.0.3",
    "eslint": "~8.33.0",
    "git-notify": "~0.2.3",
    "mocha": "~10.2.0",
    "nock": "~13.3.0",
    "nyc": "~15.1.0",
    "sinon": "~15.0.0",
    "sinon-chai": "~3.7.0",
    "ts-node": "~10.9.1",
    "tsc-alias": "~1.8.2",
    "tsconfig-paths": "~4.1.2",
    "typescript": "~4.9.5"
  },
  "engines": {
    "node": "18.14.0",
    "npm": "9.3.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/test/**/*.ts",
      "**/configuration/**/*.ts",
      "**/start.ts",
      "**/setup.ts",
      "**/src/**/tasks/*",
      "**/src/evenements/index.ts",
      "**/src/logements/index.ts",
      "**/src/stages/index.ts",
      "**/infrastructure/**/node-file-system.client.ts",
      "**/infrastructure/**/country-to-iso.ts",
      "**/infrastructure/**/html-to-markdown.sanitizer.ts",
      "**/infrastructure/**/uuid.generator.ts",
      "**/infrastructure/**/unzip.client.ts",
      "**/infrastructure/**/feature-flipping-*.ts",
      "**/src/cli/**/*.ts",
      "**/tool/*"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
